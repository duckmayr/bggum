citHeader("To cite ggumR in publications use:")

citEntry(entry = "Manual",
  title        = "Generalized Graded Unfolding Model for R",
  author       = personList(as.person("JB Duck-Mayr"),
                   as.person("Patrick Cunha Silva"),
                   as.person("Luwei Ying")),
  year         = "2017",
  note         = "R package version 0.1",

  textVersion  =
  paste("JB Duck-Mayr, Patrick Cunha Silva, and Luwei Ying. 2017.",
        "Generalized Graded Unfolding Model for R.",
        "R package version 0.1.")
)
